<?xml version="1.0" encoding="UTF-8"?>
<TabPage xmlns="clr-namespace:Eto.Forms;assembly=Eto" Text="Place Objects">
    
    <StackLayout>
        <Label Text="Place Units For Player:" />
        
        <TableLayout>
            <TableRow>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset0" Text="P0" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset1" Text="P1" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click"/>
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset2" Text="P2"  Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click"/>
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset3" Text="P3" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset4" Text="P4" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
            </TableRow>
            <TableRow>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset5" Text="P5" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset6" Text="P6" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click"/>
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset7" Text="P7" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset8" Text="P8" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click"/>
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="pset9" Text="P9" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click" />
                        <TableCell />
                    </TableRow>
                </TableLayout>
                <TableLayout>
                    <TableRow>
                        <Button ID="psetscav" Text="Scav" Style="toggle" PreLoad="AnyPlayer_PreLoad" Click="AnyPlayer_Click"/>
                        <TableCell />
                    </TableRow>
                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout>
            <TableRow>
                <Label Text="Tool:"/>
                
                <TableLayout>
                    <TableRow>
                        <Button ID="cmdPlaceOne" Text="Place One Object" Click="ToolSelection_Click" />
                    </TableRow>
                    <TableRow>
                        <Button ID="cmdPlaceRow" Text="Place Row Of Objects" Click="ToolSelection_Click" />
                    </TableRow>

                </TableLayout>
                
            </TableRow>

        </TableLayout>
        
        
        <TableLayout>
            <TableRow>
                <Label Text="Rotation:" />

                <TableLayout>
                    <TableRow>
                        <TableLayout>
                            <TableRow>
                                <NumericUpDown ID="nRotation" MinValue="0" MaxValue="359" />
                                <TableCell />
                                <CheckBox ID="chkRandom" Text="Random" />
                            </TableRow>
                        </TableLayout>
                    </TableRow>
                    <TableRow>
                        <CheckBox ID="chkRotateFootprints" Text="Rotate Footprints" />
                    </TableRow>
                    <TableRow>
                        <CheckBox ID="chkAutoWalls" Text="Automatic Walls" />
                    </TableRow>
                </TableLayout>

            </TableRow>
        </TableLayout>


        <StackLayoutItem Expand="True" HorizontalAlignment="Stretch">
            <StackLayoutItem.Control>
                <TabControl >
                    <TabPage Text="Features">
                        <StackLayout>
                            <TableLayout>
                                <TableRow>
                                    <Label Text="Filter:" />
                                    <SearchBox />
                                </TableRow>
                            </TableLayout>
                            <GridView ID="gFeatures"/>
                        </StackLayout>
                    </TabPage>
                    <TabPage Text="Structures">

                        <TableLayout>
                            <TableRow>
                                <Label Text="Filter:" />
                                <SearchBox />
                            </TableRow>
                            <TableRow>
                                <GridView ID="gStructures" />
                            </TableRow>
                        </TableLayout>

                    </TabPage>
                    <TabPage Text="Droids">

                        <TableLayout>
                            <TableRow>
                                <Label Text="Filter:" />
                                <SearchBox />
                            </TableRow>
                            <TableRow>
                                <GridView ID="gDroids" />
                            </TableRow>
                        </TableLayout>

                    </TabPage>
                </TabControl>
            </StackLayoutItem.Control>
        </StackLayoutItem>

    </StackLayout>

</TabPage>