<?xml version="1.0" encoding="UTF-8"?>
<eto:Dialog
	   xmlns="http://schema.picoe.ca/eto.forms" 
	   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:sys="clr-namespace:System;assembly=mscorlib"
       xmlns:eto="clr-namespace:Eto.Forms;assembly=Eto"
       xmlns:s="clr-namespace:SharpFlame.Gui.Controls;assembly=SharpFlame"
       Title="Compile Map">

    <eto:TableLayout>
        <eto:TableRow>
            <eto:TableCell>
                <eto:ImageView ID="imgLogo" />
            </eto:TableCell>
        </eto:TableRow>
        <eto:TableRow>
            <eto:TableCell>
                <eto:Label ID="lblName" HorizontalAlign="Center" />
            </eto:TableCell>
        </eto:TableRow>
        <eto:TableRow>
            <eto:TableCell>
                <eto:Label ID="lblVersion" HorizontalAlign="Center" />
            </eto:TableCell>
        </eto:TableRow>
        <eto:TableRow>
            <eto:TableCell>
                <eto:Label ID="lblCopy0" Text="Originally by Flail13" HorizontalAlign="Center" />
            </eto:TableCell>
        </eto:TableRow>
        <eto:TableRow>
            <eto:TableCell>
                <eto:Label ID="lblCopy1" Text="Copyright 2014 NF-Cowboy, pcdummy, jorzi" HorizontalAlign="Center" />
            </eto:TableCell>
        </eto:TableRow>
        <eto:TableRow>
            <eto:TableCell>                            
                <eto:TableLayout>
                    <eto:TableRow ScaleHeight="True" />
                    <eto:TableRow>
                        <eto:TableCell ScaleWidth="True" />
                        <eto:TableCell>
                            <eto:Button ID="cmdClose" Text="Close" Click="cmdClose_Click" />
                        </eto:TableCell>
                        <eto:TableCell ScaleWidth="True" />
                    </eto:TableRow>
                    <eto:TableRow ScaleHeight="True"/>
                </eto:TableLayout>
            </eto:TableCell>
        </eto:TableRow>
    </eto:TableLayout>
</eto:Dialog>